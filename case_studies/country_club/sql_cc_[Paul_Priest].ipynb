  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "5f4d8993-7e61-47e0-97da-6a72e14e3630",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "[('Table Tennis', 180), ('Snooker Table', 240), ('Pool Table', 270)]"
      ]
